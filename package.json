  "license": "MIT",
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^1.10.3",
    "leaflet": "~0.7.5",
    "mocha": "^2.4.5",
    "mocha-phantomjs": "^4.0.2",
    "mversion": "^1.10.1",
    "phantomjs-prebuilt": "^2.1.4"
  },
  "peerDependencies": {
    "chai": "^3.5.0"
  }
