{
  "name": "chai-leaflet",
  "version": "0.0.8",
  "description": "Chai assertions to use with Leaflet map apps",
  "main": "chai-leaflet.js",
  "scripts": {
    "test": "npm run lint && npm run test-browser",
    "lint": "eslint chai-leaflet.js",
    "test-node": "mocha test/test.js",
    "test-browser": "mocha-phantomjs test/index.html",
    "release": "mversion patch -m"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jieter/chai-leaflet.git"
  },
  "keywords": [
    "chai",
    "testing",
    "assertions",
    "leaflet",
    "map",
    "gis"
  ],
  "author": "Jan Pieter Waagmeester <jieter@jieter.nl>",
  "license": "MIT",
  "devDependencies": {
    "chai": "~1.8.1",
    "eslint": "^0.15.0",
    "leaflet": "~0.7.0",
    "mocha": "^2.2.1",
    "mocha-phantomjs": "^3.5.3",
    "mversion": "^1.10.0",
    "phantomjs": "^1.9.16"
  },
  "peerDependencies": {
    "chai": ">= 1.8.1 < 2"
  }
}
