{
  "rules": {
    "no-mixed-spaces-and-tabs": [2, "smart-tabs"],
    "indent": [2, "tab", {"VariableDeclarator": 0}],
    "curly": 2,
    "spaced-comment": 2,
    "strict": 0,
    "wrap-iife": 0,
    "key-spacing": 0
  },
  "globals": {
    "L": true,
    "chai": true,
    "module": true
  },
  "env": {
    "browser": true,
    "amd": true
  }
}
